{
    "name": "yagpt",
    "version": "0.0.23",
    "description": "A tmplate manager for projects",
    "main": "index.js",
    "scripts": {
        "start": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/index.ts",
        "start:watch": "./node_modules/.bin/nodemon",
        "build": "rimraf dist && tsc",
        "build:watch": "rimraf dist && tsc -w",
        "coverage": "nyc mocha --opts mocha.coverage.opts",
        "coverage:watch": "DMTPL_ENV=testing nodemon --config nodemon.coverage.json",
        "coverage:chrome": "nyc mocha --opts mocha.coverage.opts && google-chrome coverage/index.html",
        "chrome:coverage": "google-chrome coverage/index.html",
        "debug": "dotenv -- nodemon -e ts -x ts-node --inspect src/index.ts",
        "dev": "npm-run-all --parallel start",
        "lint": "./node_modules/.bin/tslint 'src/**/*.ts'",
        "lint:watch": "nodemon --config nodemon.lint.json",
        "test:programatically:watch": "nodemon --config nodemon.mocha.json",
        "test:programatically": "./node_modules/.bin/ts-node -r tsconfig-paths/register -r source-map-support/register ./src/utils/mocha/run.ts",
        "prod": "node dist/index.js",
        "test": "npm run test:src",
        "test:src": "nyc mocha --opts mocha.src.opts",
        "test:src:watch": "mocha --opts mocha.src.opts --watch",
        "test:watch": "DMTPL_ENV=testing npm run test:src:watch"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/divramod/yagpt.git"
    },
    "author": "Arvid Petermann",
    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/divramod/yagpt/issues"
    },
    "homepage": "https://gitlab.com/divramod/yagpt#README",
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "**/*.d.ts",
            "node_modules/**",
            "test-assets/**",
            "src/**/*.spec.ts"
        ],
        "reporter": [
            "text",
            "html"
        ],
        "all": true
    },
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/expect": "^1.20.3",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.5.7",
        "chai": "^4.1.2",
        "chokidar": "^2.0.4",
        "chokidar-cli": "^1.2.0",
        "dotenv-cli": "^1.4.0",
        "expect": "^23.5.0",
        "istanbul": "^0.4.5",
        "mocha": "^5.2.0",
        "mocha-clean": "^1.0.0",
        "mock-fs": "^4.6.0",
        "mock-stdin": "^0.3.1",
        "nodemon": "^1.18.3",
        "npm-run-all": "^4.1.3",
        "nyc": "^12.0.2",
        "rimraf": "^2.6.2",
        "source-map-support": "^0.5.6",
        "ts-mocha": "^2.0.0",
        "ts-node": "^7.0.0",
        "tsconfig-paths": "^3.5.0",
        "tslint": "^5.11.0",
        "typescript": "^3.0.1",
        "typescript-eslint-parser": "^17.0.1",
        "underscore": "^1.9.1"
    },
    "dependencies": {
        "branch-name": "^1.0.0",
        "colors": "^1.3.1",
        "commander": "^2.17.1",
        "global": "^4.3.2",
        "inquirer": "^6.0.0",
        "moment": "^2.22.2",
        "semver": "^5.5.1",
        "shelljs": "^0.8.2",
        "simple-git": "^1.96.0",
        "terminal-link": "^1.1.0"
    },
    "bin": {
        "yagpt": "./bin/yagpt",
        "yg": "./bin/yagpt"
    }
}
