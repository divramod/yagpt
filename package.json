{
  "name": "dm-tpl",
  "version": "0.0.17",
  "description": "A tmplate manager for projects",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/ts-node -r tsconfig-paths/register src/index.ts",
    "start:watch": "./node_modules/.bin/nodemon",
    "start1": "dotenv -- nodemon -e ts -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start",
    "prod": "node dist/index.js",
    "debug": "dotenv -- nodemon -e ts -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "build:watch": "rimraf dist && tsc -w",
    "lint": "./node_modules/.bin/tslint 'src/**/*.ts'",
    "lint:watch": "./node_modules/.bin/chokidar src/**/*.ts -c \"./node_modules/.bin/tslint 'src/**/*.ts'\" --initial --verbose",
    "test:src": "nyc mocha --opts mocha.src.opts",
    "test:src:watch": "mocha --opts mocha.src.opts --watch",
    "test:dist": "nyc mocha --opts mocha.dist.opts",
    "test:dist:watch": "mocha --opts mocha.dist.opts --watch",
    "test": "npm run test:src",
    "test:watch": "npm run test:src:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/divramod/dm-tpl.git"
  },
  "author": "Arvid Petermann",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/divramod/dm-tpl/issues"
  },
  "homepage": "https://gitlab.com/divramod/dm-tpl#README",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/expect": "^1.20.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "chai": "^4.1.2",
    "chokidar": "^2.0.4",
    "chokidar-cli": "^1.2.0",
    "dotenv-cli": "^1.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mock-fs": "^4.6.0",
    "mock-stdin": "^0.3.1",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.6",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^17.0.1",
    "underscore": "^1.9.1"
  },
  "dependencies": {
    "colors": "^1.3.1",
    "commander": "^2.17.1",
    "global": "^4.3.2",
    "inquirer": "^6.0.0",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2"
  },
  "bin": {
    "dm-tpl": "./bin/dm-tpl",
    "dmTpl": "./bin/dm-tpl",
    "dmp": "./bin/dm-tpl"
  },
  "projects": [
    {
      "name": "specific",
      "path": "example/project/specific",
      "type": "specific",
      "entrypoint": "index.ts",
      "active": "true"
    },
    {
      "name": "tpl-server-nodejs-workers",
      "path": "example/project/tpl-server-nodejs-workers",
      "type": "tpl",
      "upstream": "git@gitlab.com:divramod/tmpl-server-nodejs-workers.git",
      "version": "0.0.1",
      "active": "true"
    }
  ]
}